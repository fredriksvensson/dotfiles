[include]
    path = ./profiles/git-personal.conf
[includeIf "gitdir:~/work/"]
    path = ./profiles/git-client-a.conf

[user]
  signingkey = ssh-rsa 

[gpg]
  format = ssh

[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

[commit]
  gpgsign = true

[push]
	autoSetupRemote = true
